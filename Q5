#include <iostream>
using namespace std;

int main() {
    int arr[10], count = 0;
    cout << "Enter 10 numbers: ";
    for (int i = 0; i < 10; i++) {
        cin >> arr[i];
        if (arr[i] >= 10 && arr[i] <= 99 && arr[i] % 2 == 0)
            count++;
    }
    cout << "Count of two-digit even numbers = " << count;
    return 0;
}
